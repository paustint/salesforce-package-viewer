{
  "manifest_version": 3,
  "name": "Salesforce Package Version Viewer",
  "description": "View all 2GP managed and unmanaged package versions.",
  "version": "1.0.0",
  "action": {
    "default_icon": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "js/backgroundPage.js"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "host_permissions": ["https://*.salesforce.com/*", "http://localhost:1234/*"],
  "permissions": ["tabs", "storage"]
}
